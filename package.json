{
  "name": "bumper",
  "version": "19.4.0-dev.0",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/bumper.git"
  },
  "devDependencies": {
    "grunt": "~1.1.0"
  },
  "phet": {
    "requirejsNamespace": "BUMPER",
    "phetLibs": [
      "chains"
    ],
    "runnable": true,
    "simulation": true,
    "supportedBrands": [
      "phet",
      "phet-io",
      "adapted-from-phet"
    ],
    "ignoreForAutomatedMaintenanceReleases": true,
    "phet-io": {
      "validation": true
    }
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js"
  }
}
